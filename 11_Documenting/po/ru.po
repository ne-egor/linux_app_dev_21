#: guesser.c:11 guesser.c:16
#, c-format
msgid "Usage: %s [OPTION...]\n"
msgstr "Использование: %s [Опция...]\n"

#: guesser.c:12
#, c-format
msgid "Try `%s --help' or `%s -?' for more information\n"
msgstr "Попробуйте `%s --help' or `%s -?' для более подробной информации\n"

#: guesser.c:17
#, c-format
msgid ""
"Guesser -- a program which guesses number from 1 to 100\n"
"\n"
msgstr ""
"Guesser -- программа, которая угадывает число от 1 до 100\n"
"\n"

#: guesser.c:18
#, c-format
msgid "\t-r          Display numbers in Roman\n"
msgstr "\t-r          Показывать числа в римской системе счисления\n"

#: guesser.c:19
#, c-format
msgid "\t--help      Give this help list\n"
msgstr "\t--help      Показать все опции\n"

#: guesser.c:21
#, c-format
msgid "Report bugs to <nowhere@gmail.com\n"
msgstr "Отправляйте баги на <nowhere@gmail.com\n"

#: guesser.c:35
#, c-format
msgid "Is your number less then %s?\n"
msgstr "Ваше число меньше %s?\n"

#: guesser.c:37
#, c-format
msgid "Is your number less then %d?\n"
msgstr "Ваше число меньше %d?\n"

#: guesser.c:42
msgid "no"
msgstr "нет"

#: guesser.c:73
#, c-format
msgid "I will try to guess a number from I to C\n"
msgstr "Я попытаюсь угадать число от I до C\n"

#: guesser.c:74
#, c-format
msgid "Your number is %s\n"
msgstr "Ваше число равно %s\n"

#: guesser.c:76
#, c-format
msgid "I will try to guess a number from 1 to 100\n"
msgstr "Я попытаюсь угадать число от 1 до 100\n"

#: guesser.c:77
#, c-format
msgid "Your number is %d\n"
msgstr "Ваше число равно %d\n"

